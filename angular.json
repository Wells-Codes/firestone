						"lintFilePatterns": [
							"libs/shared/data-access/api-runner/src/**/*.ts",
							"libs/shared/data-access/api-runner/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/shared/data-access/api-runner"],
					"options": {
						"jestConfig": "libs/shared/data-access/api-runner/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"shared-data-access-storage": {
			"projectType": "library",
			"root": "libs/shared/data-access/storage",
			"sourceRoot": "libs/shared/data-access/storage/src",
			"prefix": "lib",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/data-access/storage/src/**/*.ts",
							"libs/shared/data-access/storage/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/shared/data-access/storage"],
					"options": {
						"jestConfig": "libs/shared/data-access/storage/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"shared-feature-overwolf": {
			"projectType": "library",
			"root": "libs/shared/feature/overwolf",
			"sourceRoot": "libs/shared/feature/overwolf/src",
			"prefix": "lib",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/feature/overwolf/src/**/*.ts",
							"libs/shared/feature/overwolf/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/shared/feature/overwolf"],
					"options": {
						"jestConfig": "libs/shared/feature/overwolf/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"shared-feature-preferences": {
			"projectType": "library",
			"root": "libs/shared/feature/preferences",
			"sourceRoot": "libs/shared/feature/preferences/src",
			"prefix": "lib",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/feature/preferences/src/**/*.ts",
							"libs/shared/feature/preferences/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/shared/feature/preferences"],
					"options": {
						"jestConfig": "libs/shared/feature/preferences/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"schematics": {
		"@nrwl/angular:application": {
			"style": "css",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "css",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "css"
		}
	}
}
