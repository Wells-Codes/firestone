{
	"version": 1,
	"projects": {
		"firestone": {
			"projectType": "application",
			"root": "apps/firestone",
			"sourceRoot": "apps/firestone/src",
			"prefix": "firestone",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/firestone",
						"index": "apps/firestone/src/index.html",
						"main": "apps/firestone/src/main.ts",
						"polyfills": "apps/firestone/src/polyfills.ts",
						"tsConfig": "apps/firestone/tsconfig.app.json",
						"assets": ["apps/firestone/src/favicon.ico", "apps/firestone/src/assets"],
						"styles": ["apps/firestone/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/firestone/src/environments/environment.ts",
									"with": "apps/firestone/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "firestone:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "firestone:build:production"
						},
						"development": {
							"browserTarget": "firestone:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "firestone:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/firestone/src/**/*.ts", "apps/firestone/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/firestone"],
					"options": {
						"jestConfig": "apps/firestone/jest.config.js",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"firestone-e2e": {
			"root": "apps/firestone-e2e",
			"sourceRoot": "apps/firestone-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/firestone-e2e/cypress.json",
						"devServerTarget": "firestone:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "firestone:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/firestone-e2e/**/*.{js,ts}"]
					}
				}
			},
			"tags": [],
			"implicitDependencies": ["firestone"]
		},
		"notifications": {
			"projectType": "application",
			"root": "apps/notifications",
			"sourceRoot": "apps/notifications/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/notifications",
						"index": "apps/notifications/src/index.html",
						"main": "apps/notifications/src/main.ts",
						"polyfills": "apps/notifications/src/polyfills.ts",
						"tsConfig": "apps/notifications/tsconfig.app.json",
						"aot": true,
						"assets": ["apps/notifications/src/favicon.ico", "apps/notifications/src/assets"],
						"styles": ["apps/notifications/src/styles.css"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/notifications/src/environments/environment.ts",
									"with": "apps/notifications/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "notifications:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "notifications:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "notifications:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/notifications/src/**/*.ts", "apps/notifications/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/notifications"],
					"options": {
						"jestConfig": "apps/notifications/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"notifications-e2e": {
			"root": "apps/notifications-e2e",
			"sourceRoot": "apps/notifications-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/notifications-e2e/cypress.json",
						"tsConfig": "apps/notifications-e2e/tsconfig.e2e.json",
						"devServerTarget": "notifications:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "notifications:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/notifications-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"notifications-feature-shell": {
			"projectType": "library",
			"root": "libs/notifications/feature-shell",
			"sourceRoot": "libs/notifications/feature-shell/src",
			"prefix": "lib",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/notifications/feature-shell/src/**/*.ts",
							"libs/notifications/feature-shell/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/notifications/feature-shell"],
					"options": {
						"jestConfig": "libs/notifications/feature-shell/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"shared-data-access-api-runner": {
			"projectType": "library",
			"root": "libs/shared/data-access/api-runner",
			"sourceRoot": "libs/shared/data-access/api-runner/src",
			"prefix": "lib",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
